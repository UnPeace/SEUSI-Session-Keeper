{
  "manifest_version": 2,
  "name": "Seusi Session Keeper",
  "version": "1.0",
  "description": "It's a maid that will keep your browsing data and serve back to its master.",
  "icons": {
  "48": "icon-48.png"
  },
  "permissions": [
    "cookies",
    "storage",
    "downloads",
    "tabs",
    "identity",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "oauth2": {
    "client_id": "143594037740-80egr34dcshqjs82123dvja6utms6bsk.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.file"
    ]
  }
}
